        .dropdown .dropdown-menu {
          display: none;
          position: absolute;
          top: 100%;
          left: 0;
          background-color: #f9f9f9;
          padding: 10px;
          list-style-type: none;
          margin: 0;
          box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
        }
      
        .dropdown:hover .dropdown-menu {
          display: block;
        }
      
        .dropdown-menu li {
          margin-bottom: 5px;
        }
      
        .dropdown-menu a {
          display: block;
          padding: 5px 10px;
          color: #333;
          text-decoration: none;
        }
      
        .dropdown-menu a:hover {
